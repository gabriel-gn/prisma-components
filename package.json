  },
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm i node-sass && node scripts/update-color-themes.js && npm run build:lib && npm uninstall node-sass",
    "build:lib": "node scripts/remove-styles-from-build.js && ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "storybook:ng": "npm run docs:json && ng run lib:storybook ",
    "build-storybook": "npm run docs:json && build-storybook",
    "build-storybook-dist": "npm run build-storybook -- -o ./dist/storybook",
    "deploy-npm": "cd dist/components && npm publish --access=public"
  },
  "private": false,
  "dependencies": {
