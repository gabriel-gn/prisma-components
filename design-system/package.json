{
  "name": "design-system",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006 -s ./.storybook/public --no-manager-cache",
    "build-storybook": "npm run docs:json && build-storybook",
    "build-storybook-dist": "npm run build-storybook -- -o ./storybook-dist -s ./.storybook/public",
    "build-npm": "ng build --project=lib --prod",
    "deploy-npm": "cd dist/lib && npm publish --access=public",
    "deploy-gpr": "cd dist/lib && npm publish"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~11.2.10",
    "@angular/cdk": "^11.2.9",
    "@angular/common": "~11.2.10",
