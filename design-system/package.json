    "@angular/platform-browser-dynamic": "~12.2.15",
    "@angular/router": "~12.2.15",
    "@geometricpanda/storybook-addon-badges": "0.2.1",
    "@storybook/addon-docs": "^6.5.0-alpha.4",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.5.0-alpha.4",
