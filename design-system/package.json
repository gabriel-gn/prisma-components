    "@angular/core": "~11.2.10",
    "@angular/forms": "~11.2.10",
    "@angular/material": "^11.2.9",
    "@angular/platform-browser": "~11.2.10",
    "@angular/platform-browser-dynamic": "~11.2.10",
    "@angular/router": "~11.2.10",
    "@geometricpanda/storybook-addon-badges": "0.2.0",
    "@storybook/addon-docs": "^6.4.0-alpha.18",
    "@storybook/addon-storysource": "^6.4.0-alpha.18",
    "@storybook/addons": "^6.4.0-alpha.18",
    "@storybook/api": "^6.4.0-alpha.18",
    "@storybook/components": "^6.4.0-alpha.18",
