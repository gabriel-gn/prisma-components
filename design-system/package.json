{
  "name": "design-system",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabriel-gn/prisma-components.git"
  },
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006 -s ./.storybook/public --no-manager-cache",
    "build-storybook": "npm run docs:json && build-storybook",
    "build-storybook-dist": "npm run build-storybook -- -o ./storybook-dist -s ./.storybook/public",
    "build-npm": "npm i node-sass && node scripts/update-color-themes.js && ng build --project=lib --configuration production && npm uninstall node-sass",
    "deploy-npm": "cd dist/components && npm publish --access=public",
    "deploy-gpr": "cd dist/components && npm publish"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~12.2.15",
    "@angular/cdk": "^11.2.9",
    "@angular/common": "~12.2.15",
