    "@storybook/components": "^6.5.0-alpha.4",
    "@storybook/core-events": "^6.5.0-alpha.4",
    "angular2-hotkeys": "^2.4.0",
    "autoprefixer": "^10.3.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "react": "^16.14.0",
    "rxjs": "~6.6.0",
    "storybook-ng-source-addon": "0.0.18",
